.SILENT:

clean:
	echo "Cleaning up interface area (vmw/interface/slic)..."
	rm -f *~ *.o main_define.v
	rm -f *.area *.timing *.log *.aux 
	rm -rf WORK main_define.v

realclean: clean
	echo "Really cleaning up interface area (vmw/interface/slic)..."
	rm -f *_vmw.v

